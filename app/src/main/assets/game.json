  "Question": [
    {
      "mIdQuestion": "1",
      "mContentQuestion": "⁅⁆ là khoa học nghiên cứu các chất, sự biến đổi và ứng dụng.",
      "mIdLevel": "1",
      "mIdBlock": "8",
      "mIdType": "2",
      "mExtent": "0"
    },
    {
      "mIdQuestion": "2",
      "mContentQuestion": "Hóa học có vai trò ⁅⁆ trong cuộc sống chúng ta.",
      "mIdLevel": "1",
      "mIdBlock": "8",
      "mIdType": "2",
      "mExtent": "0"
    },
    {
      "mIdQuestion": "3",
      "mContentQuestion": "Chất có khắp mọi nơi, ở đâu có vật thể ở đó có ⁅⁆.",
      "mIdLevel": "1",
      "mIdBlock": "8",
      "mIdType": "2",
      "mExtent": "0"
    },
    {
      "mIdQuestion": "4",
      "mContentQuestion": "Nước ⁅⁆ gồm nhiều chất trộn lẫn là một ⁅⁆.",
      "mIdLevel": "1",
      "mIdBlock": "8",
      "mIdType": "2",
      "mExtent": "0"
    },
    {
      "mIdQuestion": "5",
      "mContentQuestion": "⁅⁆ là chất tinh khiết.",
      "mIdLevel": "1",
      "mIdBlock": "8",
      "mIdType": "2",
      "mExtent": "0"
    },
    {
      "mIdQuestion": "6",
      "mContentQuestion": "Dựa vào sự ⁅⁆ về tính chất ⁅⁆ có thể tách một chất ra khỏi ⁅⁆.",
      "mIdLevel": "1",
      "mIdBlock": "8",
      "mIdType": "2",
      "mExtent": "0"
    },
    {
      "mIdQuestion": "7",
      "mContentQuestion": "Nguyên tử là hạt vô cùng ⁅⁆ và trung hòa về ⁅⁆.",
      "mIdLevel": "1",
      "mIdBlock": "8",
      "mIdType": "2",
      "mExtent": "0"
    },
    {
      "mIdQuestion": "8",
      "mContentQuestion": "Nguyên tử là hạt nhân mang ⁅⁆ và vỏ tạo bởi một hay nhiều ⁅⁆ mang ⁅⁆.",
      "mIdLevel": "1",
      "mIdBlock": "8",
      "mIdType": "2",
      "mExtent": "0"
    },
    {
      "mIdQuestion": "9",
      "mContentQuestion": "Hạt nhân tạo bởi ⁅⁆ và nơtron.",
      "mIdLevel": "1",
      "mIdBlock": "8",
      "mIdType": "2",
      "mExtent": "0"
    },
    {
      "mIdQuestion": "10",
      "mContentQuestion": "Trong mỗi nguyên, số proton bằng số ⁅⁆.",
      "mIdLevel": "1",
      "mIdBlock": "8",
      "mIdType": "2",
      "mExtent": "0"
    },
    {
      "mIdQuestion": "11",
      "mContentQuestion": "Hiện tượng chất biến đổi mà vẫn giữ nguyên là chất ⁅⁆, được gọi là hiện tượng ⁅⁆.",
      "mIdLevel": "2",
      "mIdBlock": "8",
      "mIdType": "2",
      "mExtent": "0"
    },
    {
      "mIdQuestion": "12",
      "mContentQuestion": "Hiện tượng chất biến đổi có tạo ra chất khác, được gọi là hiện tượng ⁅⁆.",
      "mIdLevel": "2",
      "mIdBlock": "8",
      "mIdType": "2",
      "mExtent": "0"
    },
    {
      "mIdQuestion": "13",
      "mContentQuestion": "Quá trình biến đổi từ chất này thành chất khác gọi là ⁅⁆.",
      "mIdLevel": "2",
      "mIdBlock": "8",
      "mIdType": "2",
      "mExtent": "0"
    },
    {
      "mIdQuestion": "14",
      "mContentQuestion": "Trong phản ứng hóa học chỉ có ⁅⁆ giữa các nguyên tử thay đổi làm cho phân tử này biến đổi thành ⁅⁆ khác.",
      "mIdLevel": "2",
      "mIdBlock": "8",
      "mIdType": "2",
      "mExtent": "0"
    },
    {
      "mIdQuestion": "15",
      "mContentQuestion": "Phản ứng xảy ra được khi các ⁅⁆ tiếp xúc với nhau, có trường hợp đun nóng, có trường hợp cần chất xúc tác, … .",
      "mIdLevel": "2",
      "mIdBlock": "8",
      "mIdType": "2",
      "mExtent": "0"
    },
    {
      "mIdQuestion": "16",
      "mContentQuestion": "Nhận biết ⁅⁆ xảy ra dựa vào dấu hiệu có chất mới tạo thành",
      "mIdLevel": "2",
      "mIdBlock": "8",
      "mIdType": "2",
      "mExtent": "0"
    },
    {
      "mIdQuestion": "17",
      "mContentQuestion": "Định luật: \"Trong một ⁅⁆, tổng ⁅⁆ của các sản phẩm bằng tổng khối lượng các chất ⁅⁆ phản ứng.",
      "mIdLevel": "2",
      "mIdBlock": "8",
      "mIdType": "2",
      "mExtent": "0"
    },
    {
      "mIdQuestion": "18",
      "mContentQuestion": "Áp dụng: Trong một phản ứng có n chất, kể cả chất phản ứng và ⁅⁆, nếu biết ⁅⁆ của (n-1) chất thì tính được khối lượng của các chất còn lại.",
      "mIdLevel": "2",
      "mIdBlock": "8",
      "mIdType": "2",
      "mExtent": "0"
    },
    {
      "mIdQuestion": "19",
      "mContentQuestion": "⁅⁆ hóa học biểu diễn ngắn gọn ⁅⁆ hóa học.",
      "mIdLevel": "2",
      "mIdBlock": "8",
      "mIdType": "2",
      "mExtent": "0"
    },
    {
      "mIdQuestion": "20",
      "mContentQuestion": "Phương trình hóa học cho biết ⁅⁆ về số nguyên tử, số  ⁅⁆ giữa các chất cũng như từng cặp chất trong phản ứng.",
      "mIdLevel": "2",
      "mIdBlock": "8",
      "mIdType": "2",
      "mExtent": "0"
    },
    {
